#include <stdio.h>
#include <stdlib.h>

void exampleWhileLoop() {
    int x = 1;
    while (x <= 5) {
        printf("%d \n", x);
        x++;
    }
}

// Example of do-while loop
void exampleDoWhileLoop() {
    int y = 1;
    do {
        printf("%d ", y);
        y++;
    } while (y <= 5);
    // Output: 1 2 3 4 5
}
